[Unit]
Description="RTSP video stream client"
After=network.target

[Service]
Type=simple
User=sad
Group=sad
Restart=always
RestartSec=2
EnvironmentFile=/enclave/drone_device_id
ExecStart=/bin/sh -c ". /opt/ros/foxy/setup_fog.sh; /usr/bin/videonode -device_id $DRONE_DEVICE_ID"

[Install]
WantedBy=multi-user.target
