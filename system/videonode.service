[Unit]
Description="RTSP video stream client"
After=network.target

[Service]
Type=simple
User=sad
Group=sad
Restart=always
RestartSec=2
EnvironmentFile=/enclave/drone_device_id
ExecStart=/bin/sh -c ". /opt/ros/foxy/setup_fog.sh; /usr/bin/videonode -stream-address \"rtsp://DroneUser:22f6c4de-6144-4f6c-82ea-8afcdf19f316@35.241.138.213:8554/$DRONE_DEVICE_ID\""

[Install]
WantedBy=multi-user.target
