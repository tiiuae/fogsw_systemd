[Unit]
Description=Odometry2
After=network-online.target
Requires=network-online.target

[Service]
Type=simple
User=sad
Group=sad
Restart=always
RestartSec=2s
ExecStart=/bin/sh -c ". /opt/ros/galactic/setup_fog.sh; ros2 launch odometry2 odometry.py"
KillSignal=SIGINT

[Install]
WantedBy=multi-user.target
